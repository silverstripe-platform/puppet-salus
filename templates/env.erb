BASEDIR="/opt/salus/data"
<% if @environment %>
ENVIRONMENT="<%= @environment %>"
<% end %>
CERT_EMAIL="<%= @cert_email %>"
ACCOUNT_EMAIL="<%= @account_email %>"
DASH_API_URL="<%= @dash_api_url %>"
DASH_API_EMAIL="<%= @dash_api_email %>"
LOCAL_CERT_STORE="<%= @local_cert_store %>"
DASH_API_TOKEN="<%= @dash_api_token %>"
AWS_KEYID="<%= @aws_keyid %>"
AWS_SECRET="<%= @aws_secret %>"
AWS_TRUST_ZONEID="<%= @aws_trust_zoneid %>"
<% if @post_nginx %>
POST_NGINX="true"
NGINX_CONFIG_TEST_CMD="<%= @nginx_config_test_cmd %>"
NGINX_RELOAD_CMD="<%= @nginx_reload_cmd %>"
<%- if @nginx_sites_config -%>
NGINX_SITES_CONFIG="<%= @nginx_sites_config %>"
<% end %>
<% end %>
<%- if @renew_days_expire -%>
RENEW_DAYS_EXPIRE=<%= @renew_days_expire %>
<% end %>
<%- if @force_renew -%>
FORCE_RENEW="true"
<% end %>
<%- if @raygun_api_key -%>
RAYGUN_API_KEY="<%= @raygun_api_key %>"
<% end %>
